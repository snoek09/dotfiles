[user]
	email = me@domain.com
	name = Name Here
	signingkey = SOMEKEYHERE
[push]
	default = simple
[alias]
	praise = blame
	ch = checkout
        pf = push --force-with-lease
	quick-stats = ! /usr/local/bin/git-quick-stats
[core]
	excludesfile = ~/.global_gitignore
	autocrlf = input
	editor = vim
[commit]
	gpgsign = true
[gpg]
	program = gpg2
